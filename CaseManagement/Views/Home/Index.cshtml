@model IEnumerable<CaseManagement.Models.Participants>

@{
    ViewBag.Title = "Dashboard";
}


<div class="row top-spacing">
   

<div class="col-lg-4 LeftHandSide">

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Caseload</h3>
    </div>
    <div class="panel-body">
        <ul class="list-group">

            @foreach (var participant in Model)
            {
                <li class="list-group-item">@Html.ActionLink(participant.Name, "Details", "Participants", new { id = participant.Id }, null)</li>

            }



        </ul>


    </div>
</div>
    </div>
<div class="col-lg-4">
<div class="panel panel-success">
    <div class="panel-heading">
        <h3 class="panel-title">Task</h3>
    </div>
    <div class="panel-body">
        Panel content
    </div>
    </div>
</div>

<div class="col-lg-4">

    <div class="panel panel-warning">
        <div class="panel-heading">
            <h3 class="panel-title">Deadlines</h3>
        </div>
        <div class="panel-body">
            Panel content
        </div>
    </div>

</div>




</div>
<div class="row">
    
</div>